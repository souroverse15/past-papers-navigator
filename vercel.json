{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/pdf-proxy",
      "dest": "/api/pdf-proxy.js"
    },
    {
      "src": "/pdfjs/(.*)",
      "dest": "/public/pdfjs/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}
